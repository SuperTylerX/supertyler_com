    "moment": "^2.30.1",
    "nuxt": "^2.14.3",
    "nuxt-fontawesome": "^0.4.0",
    "prismjs": "^1.29.0",
    "swiper": "^5.4.5",
    "vue-awesome-swiper": "^4.1.1"
  },
  "devDependencies": {
    "cross-env": "^7.0.3",
