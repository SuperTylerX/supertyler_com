{
  "name": "supertyler_com",
  "version": "1.0.0",
  "description": "A Blog Application based on Serverless Nuxt.js",
  "author": "SuperTyler",
  "private": true,
  "scripts": {
    "dev": "cross-env NODE_OPTIONS=--openssl-legacy-provider nuxt --hostname 0.0.0.0 --port 3000",
